/* components/navheader/nav-header.wxss */
.header {
    font-size: 32rpx;
    color: #666;
}
.feature {
    display: flex;
    justify-content: space-between;
    align-items: center;
    margin: 20rpx 0;
}
.feature text {
    font-size: 26rpx;
    font-weight: 700;
}
.feature .more {
    border: 1rpx solid #333;
    padding: 10rpx 20rpx;
    border-radius: 30rpx;
    font-weight: 400;
    font-size: 22rpx;
}